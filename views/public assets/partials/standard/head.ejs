<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<% 
  var __rawTitle = (typeof page_title !== 'undefined' && page_title != null) ? String(page_title) : '';
  var __suffix = __rawTitle.replace(/^\s*iWatched(\.xyz)?\s*-\s*/i,'').trim();
  var __titleOut = 'iWatched' + (__suffix ? (' - ' + __suffix) : '');
  var __desc = (typeof page_description !== 'undefined' && page_description)
    ? String(page_description)
    : 'Track, remember, and share your movies and shows. Build your complete watch history and stats.';
  var __abs = (typeof abs_url !== 'undefined' && abs_url) ? String(abs_url) : 'https://iwatched.app';
%>
<title><%= __titleOut %></title>

<!-- Favicon -->
<link rel="icon" type="image/png" href="/static/style/img/standard/cropped_ico.png">
<link rel="shortcut icon" href="/static/style/img/standard/cropped_ico.png">
<link rel="apple-touch-icon" href="/static/style/img/standard/cropped_ico.png">
<meta name="theme-color" content="#0b0f1a">

<!-- SEO: base description + Open Graph / Twitter -->
<meta name="description" content="<%= __desc %>">
<meta property="og:site_name" content="iWatched">
<meta property="og:title" content="<%= __titleOut %>">
<meta property="og:description" content="<%= __desc %>">
<meta property="og:type" content="website">
<link rel="canonical" href="<%= __abs %>">
<meta property="og:url" content="<%= __abs %>">
<meta property="og:image" content="https://iwatched.app/static/style/img/standard/standard_banner.png">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="<%= __titleOut %>">
<meta name="twitter:description" content="<%= __desc %>">
<meta name="twitter:image" content="https://iwatched.app/static/style/img/standard/standard_banner.png">

<!-- Structured data: preferred site name + logo + search action -->
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "WebSite",
  "name": "iWatched",
  "alternateName": "iWatched.app",
  "url": "https://iwatched.app",
  "potentialAction": {
    "@type": "SearchAction",
    "target": "https://iwatched.app/search?q={search_term_string}",
    "query-input": "required name=search_term_string"
  }
}
</script>
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Organization",
  "name": "iWatched",
  "url": "https://iwatched.app",
  "logo": "https://iwatched.app/static/style/img/standard/cropped_ico.png"
}
</script>

<!-- Aquire's all stylesheets -->
<link rel="stylesheet" type="text/css" href="/static/style/bootstrap/css/bootstrap.min.css">
<link rel="stylesheet" type="text/css" href="/static/style/css/stylesheet.css">
<link rel="stylesheet" type="text/css" href="/static/style/css/navigation.css">
<!-- New design fonts + base styles (th- header/footer) -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&family=Poppins:wght@500;600&display=swap" rel="stylesheet">
<link rel="stylesheet" type="text/css" href="/static/style/css/temp_home.css">
<% if (page_file === 'temp_user') { %>
  <!-- Profile page styles for new design -->
  <link rel="stylesheet" type="text/css" href="/static/style/css/temp_user.css">
<% } %>
<% if (page_file === 'temp_announcements') { %>
  <!-- Announcements page styles for new design -->
  <link rel="stylesheet" type="text/css" href="/static/style/css/temp_announcements.css">
<% } %>
<% if (typeof page_file !== 'undefined' && (page_file === 'movies' || page_file === 'shows' || page_file === 'person')) { %>
<link rel="stylesheet" type="text/css" href="/static/style/css/detail.css">
<% } %>
<% if (typeof is_admin_template !== 'undefined' && is_admin_template) { %>
<link rel="stylesheet" type="text/css" href="/static/style/css/admin_dashboard.css">
<% } %>

<!-- Cookie Consent (vanilla-cookieconsent) -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/vanilla-cookieconsent@2.9.2/dist/cookieconsent.css" crossorigin="anonymous">

<!-- Force dark theme for CookieConsent -->
<style>
  /* Apply the library's dark palette via CSS variables */
  #cc--main{
    --cc-bg:#181b1d; --cc-text:#d8e5ea;
    --cc-btn-primary-bg:#a6c4dd; --cc-btn-primary-text:#000; --cc-btn-primary-hover-bg:#c2dff7; --cc-btn-primary-hover-text:#000;
    --cc-btn-secondary-bg:#33383c; --cc-btn-secondary-text:var(--cc-text); --cc-btn-secondary-hover-bg:#3e454a; --cc-btn-secondary-hover-text:var(--cc-btn-secondary-text);
    --cc-toggle-bg-off:#667481; --cc-toggle-bg-on:var(--cc-btn-primary-bg); --cc-toggle-bg-readonly:#454c54; --cc-toggle-knob-bg:var(--cc-cookie-category-block-bg); --cc-toggle-knob-icon-color:var(--cc-bg);
    --cc-block-text:#b3bfc5; --cc-cookie-category-block-bg:#23272a; --cc-cookie-category-block-bg-hover:#2b3035; --cc-section-border:#292d31; --cc-cookie-table-border:#2b3035;
  }
  /* Make the settings button look like the dark demo */
  .cc_div #c-bns .b-tl{ background: var(--cc-btn-secondary-bg); color: var(--cc-btn-secondary-text); }
  .cc_div #c-bns .b-tl:hover{ background: var(--cc-btn-secondary-hover-bg); color: var(--cc-btn-secondary-hover-text); }
  .cc_div a{ color: inherit; opacity: .9; border-bottom-color: rgba(255,255,255,.2); }
  .cc_div a:hover{ opacity: 1; }
  /* Slightly widen buttons for mobile ergonomics */
  .cc_div .c-bn{ padding: .85em 1em; }
  /* Bottom link bar (privacy + terms), matching the demo */
  #cc-custom-links{ display:flex; gap:18px; padding: .6rem 1rem 1rem; font-size:.875rem; opacity:.9; }
  #cc-custom-links a{ border:0; text-decoration:none; opacity:.9; }
  #cc-custom-links a:hover{ opacity:1; }
  /* Make first two buttons light (Accept/Reject), third button dark */
  .cc_div #c-bns{ flex-direction: column !important; gap: .6rem; align-items: stretch !important; justify-content: flex-start !important; }
  .cc_div #c-bns .c-bn{ width: 100% !important; flex: 0 0 auto !important; display: block !important; margin: 0 !important; box-sizing: border-box; }
  #c-p-bn, #c-s-bn{
    background: var(--cc-btn-primary-bg) !important;
    color: var(--cc-btn-primary-text) !important;
  }
  #c-p-bn:hover, #c-s-bn:hover{
    background: var(--cc-btn-primary-hover-bg) !important;
    color: var(--cc-btn-primary-hover-text) !important;
  }
  #c-m-bn{
    background: var(--cc-btn-secondary-bg) !important;
    color: var(--cc-btn-secondary-text) !important;
  }
  #c-m-bn:hover{
    background: var(--cc-btn-secondary-hover-bg) !important;
    color: var(--cc-btn-secondary-hover-text) !important;
  }
  /* Force modal to bottom-left of viewport */
  .cc_div #cm{ left: 1rem !important; right: auto !important; top: auto !important; bottom: 1rem !important; margin: 0 !important; }
</style>

<!-- Google Consent Mode defaults (advanced) -->
<script>
  // Prepare dataLayer/gtag early and default all storages to denied
  window.dataLayer = window.dataLayer || [];
  function gtag(){ dataLayer.push(arguments); }
  // Default denied until user consents; keeps Google tags compliant when added later
  gtag('consent', 'default', {
    'ad_storage': 'denied',
    'analytics_storage': 'denied',
    'ad_user_data': 'denied',
    'ad_personalization': 'denied',
    'wait_for_update': 500
  });
</script>

